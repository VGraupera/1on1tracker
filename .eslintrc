{
  "env": {
    "browser": true,
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [ "react" ],
  "rules": {
      "no-underscore-dangle": 0,
      "no-console": 0,
      "arrow-body-style": 0,
      "anchor-has-content": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-boolean-value": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
  }
}
